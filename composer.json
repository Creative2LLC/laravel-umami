{
    "name": "creative2/laravel-umami",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Creative2\\Umami\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Robert Davies",
            "email": "robert@creative2.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10",
        "illuminate/http": "^9.0|^10",
        "nesbot/carbon": "^2.72"
    },
    "require-dev": {
        "pestphp/pest": "^2.33"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creative2\\Umami\\UmamiServiceProvider"
            ]
        }
    }
}
